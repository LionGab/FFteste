# Força a instalação de todas as dependências
legacy-peer-deps=false
strict-peer-dependencies=false

# Desabilita fallback automático para registry alternativo
prefer-online=true

# Garante que o package-lock.json seja respeitado
package-lock=true
package-lock-only=false

# Configurações de cache
cache-min=10
audit=false

# Engine strict (garante versão correta do Node)
engine-strict=true
